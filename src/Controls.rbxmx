						<ProtectedString name="Source"><![CDATA[local shown = false
local tween = game:GetService('TweenService')
local close = tween:Create(script.Parent.Parent, TweenInfo.new(1,Enum.EasingStyle.Bounce, Enum.EasingDirection.Out), {
	Position = UDim2.new(0.5, 0,1, -50)
})
local open = tween:Create(script.Parent.Parent, TweenInfo.new(1,Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
	Position = UDim2.new(0.5, 0,1, -script.Parent.Parent.Size.Y.Offset)
})

script.Parent.MouseButton1Click:Connect(function ()
	shown = not shown
	if shown then
		-- close
		close:Play()
	else
		-- open 
		open:Play()
	end
end)]]></ProtectedString>
